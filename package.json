{
  "name": "corporate-calendar",
  "workspaces": [
    "calendar-ui",
    "calendar-service"
  ],
  "scripts": {
    "start": "concurrently \"npm --prefix calendar-ui start\" \"npm --prefix calendar-service start\"",
    "install-all": "npm install --workspaces",
    "build": "npm run build --workspaces",
    "lint": "eslint \"calendar-service/**/*.ts\" \"calendar-ui/**/*.{ts,tsx}\" --fix",
    "lint:service": "eslint \"calendar-service/**/*.ts\" --fix",
    "lint:ui": "eslint \"calendar-ui/**/*.{ts,tsx}\" --fix"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "concurrently": "^9.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^16.0.0",
    "prettier": "^3.3.3",
    "typescript-eslint": "^8.20.0"
  }
}
